@model Department

@{
    ViewData["Title"] = "Delete";
}

<h1 class="alert alert-danger">Are You Sure to Delete This Department ! </h1>
<br />
<br />

<form asp-action="Delete">
    <div asp-validation-summary="All"></div>
    <div class="form-group">
        <label class="col col-form-label" asp-for="Code"></label>
        <input asp-for="Code" class="form-control" readonly />
    </div>
    <div class="form-group">
        <label class="col col-form-label" asp-for="Name"></label>
        <input asp-for="Name" class="form-control" readonly />
    </div>
    <div class="form-group">
        <label class="col col-form-label" asp-for="DateOfCreation"></label>
        <input asp-for="DateOfCreation" class="form-control" readonly />
    </div>

    <input type="submit" value="Delete" class="btn btn-danger" />
    <a asp-action="Index" class="btn btn-secondary">Cancel</a>
</form>

